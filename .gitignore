### .gitignore ---

# Ignore bundler config
/.bundle
/vendor/bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore incr
/incr_example

# Ignore editor files
*~
.swap

# Ignore lock files
Gemfile.lock

# Ignore assets in production mode
public/assets

# Ignore jobs output files
data/*out
data/*in

# Ignore generated files
caps/Capfile

